	<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	  <Description about="urn:mozilla:install-manifest">
		<em:id>ZooniverseXpert@jetpack</em:id>
		<em:version>1.0-night</em:version>
		<em:type>2</em:type>
		<em:bootstrap>true</em:bootstrap>
		<em:optionsType>3</em:optionsType>
		<em:optionsURL>about:zoo</em:optionsURL>
		
		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>12.0</em:minVersion> <!-- due to not sure didnt test, can maybe be lower -->
				<em:maxVersion>41.0</em:maxVersion> <!-- the latest version i tested it in -->
			</Description>
		</em:targetApplication>
		
		<!-- Front End MetaData -->
		<!-- must provide default non-localized because It's used as a default on AMO. It's used as a default by the add-on manager, with the possibility of other locales overriding it. Failure to provide a non-localized name will lead to failed upload on AMO. -->
		<em:name>ZooniverseXpert</em:name>
		<em:description>Supply an ID and see a gallery of images around it, with a form to make quick submissions</em:description>
		<em:creator>maj</em:creator>
		
		<!-- start localizing - For each entry in default data MUST have equivalent in each localized block -->
		<em:localized>
			<Description>
				<em:locale>en-US</em:locale>
				<em:name>ZooniverseXpert</em:name>
				<em:description>Supply an ID and see a gallery of images around it, with a form to make quick submissions</em:description>
				<em:creator>maj</em:creator>
			</Description>
		</em:localized>
	  </Description>
	</RDF>